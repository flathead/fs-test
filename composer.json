{
	"name": "flathead/test-theme",
	"description": "Тестовое задание для Future Search.",
	"keywords": [
		"wordpress",
		"cms"
	],
	"type": "plugin-package",
	"homepage": "https://test.flathead.ru/",
	"license": "GPL-2.0-or-later",
	"authors": [
		{
			"name": "Dmitry Guzeev",
			"homepage": "https://github.com/flathead/"
		}
	],
	"repositories": [
		{
			"type": "composer",
			"url": "https://wpackagist.org"
		}
	],
	"require": {
		"php": ">=8.1.0",
		"ext-json": "*",
		"wpackagist-plugin/cyr3lat": "^3.7",
		"wpackagist-plugin/all-in-one-seo-pack": "^4.6.2",
		"wpackagist-plugin/classic-editor": "^1.6.3",
		"wpackagist-plugin/duplicate-page": "^4.5.3"
	},
	"extra": {
		"installer-paths": {
			"wp-content/plugins/{$name}/": ["type:wordpress-plugin"]
		}
	},
	"provide": {
		"wordpress/core-implementation": "6.2.2"
	},
	"config": {
		"allow-plugins": {
			"composer/installers": true,
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
